{
    "datacenter": "you-tag",
    "data_dir": "{{ path_consul }}/data",
    "log_level": "INFO",
    "enable_syslog": true,
    "disable_update_check": true,
    "node_name" : "{{ ansible_hostname }}",
    "bind_addr": "{{ ip_consul_local }}",
    "advertise_addr": "{{ ip_consul_local }}",
    "client_addr": "{{ ip_consul_local }}",
    "ui": true,
    "server" : true,
    "bootstrap_expect": 2,
    "start_join": [{% for host in srv_hostname_consul %} "{{ host.name }}" {{ host.demiliter }} {% endfor %}]
}
